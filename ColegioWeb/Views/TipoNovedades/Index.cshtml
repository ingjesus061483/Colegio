@model IEnumerable<Factory.TipoNovedad>

@{
    ViewBag.Title = "Listado de tipos de novedades";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Crear tipo de novedad", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td> 
            @Html.ActionLink("Edatar", "Edit", new { id=item.Id })   
        </td>
    </tr>
}

</table>
