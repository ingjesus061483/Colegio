@model IEnumerable<Factory.Matricula>

@{
    ViewBag.Title = "listado de matriculas";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fechainicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fechafin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Valor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estadoMatricula.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Codigo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Fechainicio)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Fechafin)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.Valor)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.estadoMatricula.Nombre)
    </td>
    <td>        
        @Html.ActionLink("Ver detalles", "Details", new { id = item.Id }) |
       
    </td>
</tr>
}

</table>
