            @Html.DisplayNameFor(model => model.Jornada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estudiante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Valor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fechainicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fechafin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstadoMatricula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Curso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jornada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estudiante)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Valor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fechainicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fechafin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstadoMatricula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
